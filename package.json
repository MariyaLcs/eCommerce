{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "14.15."
  },
  "scripts": {
    "start": "node shop-server/server",
    "server": "nodemon shop-server/server",
    "client": "npm start --prefix shop-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node shop-server/seeder",
    "data:destroy": "node shop-server/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix shop-client && npm run build --prefix shop-client"
  },
  "author": "Masha",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
